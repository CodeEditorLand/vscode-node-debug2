		"menus": {
			"debug/callstack/context": [
				{
					"command": "extension.node-debug2.toggleSkippingFile",
					"group": "navigation",
					"when": "inDebugMode && debugType == 'node2' && callStackItemType == 'stackFrame'"
				}
			]
